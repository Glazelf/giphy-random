{
  "name": "giphy-random",
  "version": "1.0.0",
  "description": "A package to fetch a random GIF from Giphy API",
  "main": "dist/giphy-random.cjs.js",
  "module": "dist/giphy-random.esm.js",
  "browser": "dist/giphy-random.umd.js",
  "scripts": {
    "build": "NODE_ENV=build rollup -c",
    "dev": "NODE_ENV=build rollup -c -w",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "prepublishOnly": "npm run test && npm run build",
    "prettier": "prettier --write '{src,tests}/**/*.js'",
    "test": "NODE_ENV=jest jest",
    "test:coverage": "NODE_ENV=jest jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/risan/giphy-random.git"
  },
  "keywords": [
    "gif",
    "giphy",
    "fun"
  ],
  "author": "Risan Bagja Pradana <risanbagja@gmail.com> (https://risan.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/risan/giphy-random/issues"
  },
  "homepage": "https://github.com/risan/giphy-random#readme",
  "devDependencies": {
    "axios": "^0.13.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jest": "^21.2.1",
    "nock": "^9.1.4",
    "prettier": "^1.9.2",
    "rollup": "^0.52.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "peerDependencies": {
    "axios": "^0.13.1"
  }
}
